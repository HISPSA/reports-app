{
  "name": "reports-app",
  "version": "1.0.0-beta.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:style": "d2-style js check --all",
    "prestart": "npm run localize",
