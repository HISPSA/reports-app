    "react-scripts": "3.0.1",
    "react": "^16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "redux": "^4.0.1",
    "styled-jsx": "^3.2.1",
    "typeface-roboto": "^0.0.54",
    "xlsx": "^0.13.0"
  },
  "devDependencies": {
    "@dhis2/cli-style": "^3.2.2",
    "@dhis2/d2-i18n-generate": "^1.0.19",
    "enzyme-adapter-react-16": "^1.1.1",
