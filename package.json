    "@dhis2/d2-ui-sharing-dialog": "^5.3.0",
    "@dhis2/d2-ui-table": "^5.3.0",
    "@dhis2/ui": "1.0.0-beta.14",
    "chart.js": "^2.7.3",
    "connected-react-router": "^6.2.2",
    "d2": "^31.7.0",
    "d3-color": "^1.2.0",
    "final-form": "^4.11.1",
    "history": "^4.7.2",
    "lodash.debounce": "^4.0.8",
